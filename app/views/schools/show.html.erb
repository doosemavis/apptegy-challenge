<div class="page-header">
  <%= link_to schools_path, class: 'btn btn-default' do %>
    All Schools
  <% end %>
  <%= link_to edit_school_path(@school), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show school</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @school.name %></dd>
  <dt>Address:</dt>
  <dd><%= @school.address %></dd>
</dl>
